@article{pollock_breakdown_1996,
	title = {The breakdown of single-crystal solidification in high refractory nickel-base alloys},
	volume = {27},
	issn = {1073-5623, 1543-1940},
	url = {http://link.springer.com/article/10.1007/BF02649777},
	doi = {10.1007/BF02649777},
	abstract = {The breakdown of single-crystal solidification has been studied over a wide range of solidification conditions in ten superalloys with large variations in Re, Ta, and W content. Over the range of experimental conditions investigated, grain defect formation was sensitive to local thermaland solutal conditions. For a fixed alloy composition and withdrawal rate, the transition from single-crystal to equiaxed solidification did not occur abruptly. Instead, as thermal gradients were decreased in a series of experiments, isolated, highly misoriented columnar grains with the same composition as that of the base alloy developed in the presence of positive (stabilizing) thermal gradients with increasing frequency until the advance of the single-crystal front was completely blocked. The onset of columnar grain formation occurred when the primary dendrite arm spacing exceeded a critical value, corresponding to a morphological transition in the dendritic array. The onset of {\textquotedblleft}freckling{\textquotedblright} was observed at the same primary dendrite arm spacing where misoriented columnar grains began to appear. In experiments with varying levels of refractory alloy content, there was also a strong correlation between the onset of grain formation and freckle formation. These observations strongly suggest that in high refractory content superalloys, the breakdown of single-crystal solidification and the formation of misoriented grains as well as freckle-type defects are sensitively dependent on thermosolutal convection processes.},
	language = {en},
	number = {4},
	urldate = {2014-11-20},
	journal = {Metallurgical and Materials Transactions A},
	author = {Pollock, T. M. and Murphy, W. H.},
	month = apr,
	year = {1996},
	keywords = {Ceramics, Glass, Composites, Natural Methods, Materials Science, Metallic Materials, Physical Chemistry, Structural Materials},
	pages = {1081--1094},
	file = {Snapshot:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\WMQPH4BC\\Pollock and Murphy - 1996 - The breakdown of single-crystal solidification in .html:text/html}
}

@inproceedings{genereux_characterization_2000,
	address = {Warrendale, PA},
	title = {Characterization of {Freckles} in a {High} {Strength} {Wrought} {Nickel} {Superalloy}},
	url = {http://www.tms.org/superalloys/10.7449/2000/Superalloys_2000_19_27.pdf},
	abstract = {Nickel base superalloys, such as Gatorizable Waspaloy@, U-720@, 
and Rene 88@ contain relatively high weight percents of 
hardening elements (e.g. Al \& Ti), for a conventionally cast 
(VlM/VAR or VIM/ESR/VAR) and wrought (press and or rotary 
forged) product. Due to the higher volume fraction of gamma 
prime, this class of alloys has increased susceptibility for the 
formation of melt segregation defects, such as freckles. The 
propensity for freckle formation is further aggravated by the need 
for larger diameter ingots to support the trend toward higher 
thrust large diameter fan engines used on wide body twin engine 
aircraft. A review and assessment of production quality control 
techniques for freckle inspection are discussed. An in depth 
microstructural, chemical, and structural characterization is 
presented. The effects of freckle defects on key mechanical 
properties for the design of critical rotating engine components 
are shown. Finally, a brief discussion of a root cause investigation 
on the origin of freckle formation in this alloy class is given. 

Gatorizable Waspaloy@ is a trademark of United Technologies Corporation 
U-720@ is a trademark of Special Metals Company 
Rene 88@ is a trademark of the General Electric Corporation},
	publisher = {K.A. Green, M. McLean, S. Olson, and J.J. Schirra, TMS (The Minerals, Metals and Materials Society)},
	author = {Genereux, Paul D. and Borg, Christopher A.},
	year = {2000},
	pages = {19--27}
}

@article{peng_pde-based_1999,
	title = {A {PDE}-{Based} {Fast} {Local} {Level} {Set} {Method}},
	volume = {155},
	issn = {0021-9991},
	url = {http://www.sciencedirect.com/science/article/pii/S0021999199963453},
	doi = {10.1006/jcph.1999.6345},
	abstract = {We develop a fast method to localize the level set method of Osher and Sethian (1988, J. Comput. Phys.79, 12) and address two important issues that are intrinsic to the level set method: (a) how to extend a quantity that is given only on the interface to a neighborhood of the interface; (b) how to reset the level set function to be a signed distance function to the interface efficiently without appreciably moving the interface. This fast local level set method reduces the computational effort by one order of magnitude, works in as much generality as the original one, and is conceptually simple and easy to implement. Our approach differs from previous related works in that we extract all the information needed from the level set function (or functions in multiphase flow) and do not need to find explicitly the location of the interface in the space domain. The complexity of our method to do tasks such as extension and distance reinitialization is O(N), where N is the number of points in space, not O(N log N) as in works by Sethian (1996, Proc. Nat. Acad. Sci. 93, 1591) and Helmsen and co-workers (1996, SPIE Microlithography IX, p. 253). This complexity estimation is also valid for quite general geometrically based front motion for our localized method.},
	number = {2},
	urldate = {2015-02-01},
	journal = {Journal of Computational Physics},
	author = {Peng, Danping and Merriman, Barry and Osher, Stanley and Zhao, Hongkai and Kang, Myungjoo},
	month = nov,
	year = {1999},
	pages = {410--438},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\JFF45H6M\\Peng et al. - 1999 - A PDE-Based Fast Local Level Set Method.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\GDM4HJR9\\Peng et al. - 1999 - A PDE-Based Fast Local Level Set Method.html:text/html}
}

