	doi = {10.1002/nme.998},
	abstract = {A stabilized equal-order velocity{\textendash}pressure finite element algorithm is presented for the analysis of flow in porous media and in the solidification of binary alloys. The adopted governing macroscopic conservation equations of momentum, energy and species transport are derived from their microscopic counterparts using the volume-averaging method. The analysis is performed in a single domain with a fixed numerical grid. The fluid flow scheme developed includes SUPG (streamline-upwind/Petrov{\textendash}Galerkin), PSPG (pressure stabilizing/Petrov{\textendash}Galerkin) and DSPG (Darcy stabilizing/Petrov{\textendash}Galerkin) stabilization terms in a variable porosity medium. For the energy and species equations a classical SUPG-based finite element method is employed. The developed algorithms were tested extensively with bilinear elements and were shown to perform stably and with nearly quadratic convergence in high Rayleigh number flows in varying porosity media. Examples are shown in natural and double diffusive convection in porous media and in the directional solidification of a binary-alloy. Copyright {\textcopyright} 2004 John Wiley \& Sons, Ltd.},
	language = {en},
	number = {6},
	urldate = {2015-06-15},
	journal = {International Journal for Numerical Methods in Engineering},
	author = {Zabaras, Nicholas and Samanta, Deep},
	month = jun,
	year = {2004},
	keywords = {convection, Darcy flows, porous media, solidification, stabilized finite element method, volume averaging},
	pages = {1103--1138},
	file = {Full Text PDF:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\VK5KFU88\\Zabaras and Samanta - 2004 - A stabilized volume-averaging finite element metho.pdf:application/pdf;Snapshot:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\HSGW4MH2\\Zabaras and Samanta - 2004 - A stabilized volume-averaging finite element metho.html:text/html}
