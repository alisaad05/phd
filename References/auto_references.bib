	month = nov,
	year = {2009},
	keywords = {Benchmark, Binary mixture, Columnar growth, solidification, Thermosolutal convection},
	pages = {2013--2016},
	file = {ScienceDirect Snapshot:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\GHT2N7NG\\Bellet et al. - 2009 - Call for contributions to a numerical benchmark pr.html:text/html}
