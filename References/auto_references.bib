@article{zaloznik_operator_2010,
	title = {An operator splitting scheme for coupling macroscopic transport and grain growth in a two-phase multiscale solidification model: {Part} {I} {\textendash} {Model} and solution scheme},
	volume = {48},
	issn = {0927-0256},
	shorttitle = {An operator splitting scheme for coupling macroscopic transport and grain growth in a two-phase multiscale solidification model},
	url = {http://www.sciencedirect.com/science/article/pii/S0927025609001827},
	doi = {10.1016/j.commatsci.2009.04.036},
	abstract = {We present a two-phase multiscale solidification model, which accounts for coupled heat, mass, solute and momentum transfer and notably describes the motion of globular equiaxed grains. On the microscopic scale the model takes into account nucleation, finite diffusion in both solid and liquid phases, and resolves the diffusion-controlled phase change by a solute balance at the solid{\textendash}liquid interface coupled with a thermodynamic equilibrium condition for the interface. The main idea of the paper is the solution algorithm of the multiscale model. We propose and present in detail an operator splitting method that resolves the global conservation equations in two stages. In the first solution stage we resolve and integrate globally only the convection terms. In the second solution stage we resolve and integrate locally the contributions of the grain growth kinetics, i.e. the phase change and the microscopic diffusion. We analyze this splitting method based on a scaling analysis that shows that the justification of the method stems from the separation of time scales of convection, diffusion and phase change. We discuss this scale separation in detail and define a test case that is presented and analyzed in a companion paper.},
	number = {1},
	urldate = {2014-09-10},
	journal = {Computational Materials Science},
	author = {Zalo{\v z}nik, Miha and Combeau, Herv{\'e}},
	month = mar,
	year = {2010},
	keywords = {macrosegregation, Modeling, Multiphase flows, Operator splitting, solidification},
	pages = {1--10},
	file = {ScienceDirect Snapshot:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\W7WJQ5AW\\Zalo{\v z}nik and Combeau - 2010 - An operator splitting scheme for coupling macrosco.html:text/html}
}

