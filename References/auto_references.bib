	year = {2013},
	keywords = {ALE formulation, decoupling strategy, free capillary surface, mesh-moving, Navier{\textendash}Stokes, Stefan problem},
	pages = {1282--1296},
	file = {Snapshot:C\:\\Users\\RASPI\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dmti0gfw.default\\zotero\\storage\\CJ485ITZ\\B{\"a}nsch et al. - 2013 - An ALE finite element method for a coupled Stefan .html:text/html}
}

